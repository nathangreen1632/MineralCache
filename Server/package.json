{
  "name": "server",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node ./dist/server.js\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "db:migrate": "NODE_ENV=development sequelize-cli db:migrate",
    "db:undo": "NODE_ENV=development sequelize-cli db:migrate:undo",
    "db:reset": "NODE_ENV=development sequelize-cli db:migrate:undo:all && NODE_ENV=development sequelize-cli db:migrate",
    "db:seed": "NODE_ENV=development sequelize-cli db:seed:all",
    "db:seed:undo": "NODE_ENV=development sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "One Guy Productions",
  "license": "UNLICENSED",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-session": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "express-rate-limit": "^8.1.0",
    "file-type": "^21.0.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "resend": "^6.1.0",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "stripe": "^18.5.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.5.2",
    "@types/pdfkit": "^0.17.3",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "typescript": "^5.9.2"
  }
}
